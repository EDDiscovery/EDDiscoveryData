// Events: onKeyPress?(KeyPress $== Alt+F1)(cmd=topmost), onKeyPress?(KeyPress $== Alt+F2)(cmd=normalz),
// Events: onKeyPress?(KeyPress $== Alt+F3)(cmd=showintaskbar), onKeyPress?(KeyPress $== Alt+F4)(cmd=notshowintaskbar),
