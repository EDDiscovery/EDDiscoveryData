            Set v = say_%(EventClass_EventTypeStr)%(EventClass_ScanStage)
            Static %(var) = %(EventClass_ScanStage)
            Set speech = '%phrase("%indirect(v)","<;>","{{{","}}}")'
            Call speak(speech="%(speech)", QueueLimit = "4000")
Else 
    Rem print Off

